{
    "name": "fatcher",
    "version": "0.3.0",
    "main": "dist/index.cjs.js",
    "esm": "dist/index.es.js",
    "unpkg": "dist/fatcher.min.js",
    "browser": "dist/fatcher.min.js",
    "typings": "dist/index.d.ts",
    "repository": "https://github.com/fanhaoyuan/fatcher.git",
    "author": "Fansy <418536538@qq.com>",
    "description": "A out-of-box http request library with fetch for modern browsers.",
    "engines": {
        "node": ">=12.0.0"
    },
    "keywords": [
        "http",
        "fetch",
        "stream",
        "http request",
        "request"
    ],
    "files": [
        "dist",
        "README.CN.md",
        "README.md",
        "CHANGELOG.md"
    ],
    "license": "MIT",
    "scripts": {
        "sandbox": "node sandbox/server.js",
        "dev": "ts-node build/dev.ts",
        "build": "ts-node build/build.ts && tsc",
        "docs:dev": "dumi dev",
        "docs:build": "dumi build"
    },
    "gitHooks": {
        "commit-msg": "cmv",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,ts,md,json,yml}": [
            "prettier --write"
        ],
        "*.{js,ts}": [
            "eslint"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@fansy/commit-msg-validator": "^1.0.0",
        "@fansy/eslint-config": "^1.0.0",
        "@fansy/prettier-config": "^1.0.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@types/chokidar": "^2.1.3",
        "@types/koa": "^2.13.4",
        "@types/lodash": "^4.14.176",
        "@types/node": "^16.11.0",
        "@types/rimraf": "^3.0.2",
        "chalk": "^4.1.2",
        "chokidar": "^3.5.2",
        "dumi": "^1.0.13",
        "esbuild": "^0.13.11",
        "gzip-size": "^6.0.0",
        "koa": "^2.13.4",
        "koa-router": "^10.1.1",
        "lint-staged": "^12.0.3",
        "lodash": "^4.17.21",
        "rimraf": "^3.0.2",
        "rollup": "^2.58.3",
        "rollup-plugin-esbuild": "^4.6.0",
        "rollup-plugin-terser": "^7.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4",
        "koa-bodyparser": "^4.3.0",
        "koa2-cors": "^2.0.6"
    }
